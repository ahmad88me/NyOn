    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
        <owl:equivalentClass rdf:resource="https://w3id.org/def/NyOn#Individual"/>
    </owl:Class>
    


    <!-- https://schema.org/Organization -->

    <owl:Class rdf:about="https://schema.org/Organization">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/NyOn#Party_Type"/>
        <rdfs:comment xml:lang="en">organization(s) involved as the participant in the case</rdfs:comment>
        <rdfs:comment xml:lang="hi">संगठन (ओं) मामले में प्रतिभागी के रूप में शामिल है</rdfs:comment>
        <rdfs:label xml:lang="de">Organisation</rdfs:label>
        <rdfs:label xml:lang="fr">Organisation</rdfs:label>
        <rdfs:label xml:lang="es">Organización</rdfs:label>
        <rdfs:label xml:lang="en">Organization</rdfs:label>
        <rdfs:label xml:lang="hi">संगठन</rdfs:label>
    </owl:Class>
    


    <!-- https://schema.org/State -->

    <owl:Class rdf:about="https://schema.org/State">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/NyOn#Location"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/NyOn#Party_Type"/>
        <rdfs:comment xml:lang="en">represents state name/location</rdfs:comment>
        <rdfs:comment xml:lang="hi">राज्य का नाम / स्थान का प्रतिनिधित्व करता है</rdfs:comment>
        <rdfs:label xml:lang="de">Bundesland</rdfs:label>
        <rdfs:label xml:lang="es">Estado</rdfs:label>
        <rdfs:label xml:lang="en">State</rdfs:label>
        <rdfs:label xml:lang="fr">État</rdfs:label>
        <rdfs:label xml:lang="hi">राज्य</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/NyOn#Accussed -->

    <owl:Class rdf:about="https://w3id.org/def/NyOn#Accussed">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/def/NyOn#Respondent"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/def/NyOn#caseBelongsToType"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/def/NyOn#Criminal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/NyOn#Party"/>
        <rdfs:comment xml:lang="en">represents the accused of the case</rdfs:comment>
        <rdfs:comment xml:lang="hi">मामले के आरोपी का प्रतिनिधित्व करता है</rdfs:comment>
        <rdfs:label xml:lang="en">Accused</rdfs:label>
        <rdfs:label xml:lang="fr">Accusé</rdfs:label>
        <rdfs:label xml:lang="es">Acusado</rdfs:label>
        <rdfs:label xml:lang="de">Beschuldigt</rdfs:label>
        <rdfs:label xml:lang="hi">अभियुक्त</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/NyOn#Acts -->

    <owl:Class rdf:about="https://w3id.org/def/NyOn#Acts">
        <rdfs:comment xml:lang="en">Acts or Rules common for Civil and Crimnial Law</rdfs:comment>
        <rdfs:comment xml:lang="hi">नागरिक और क्रूर कानून के लिए आम अधिनियम या नियम</rdfs:comment>
        <rdfs:label xml:lang="fr">Actes</rdfs:label>
        <rdfs:label xml:lang="en">Acts</rdfs:label>
        <rdfs:label xml:lang="de">Handlung</rdfs:label>
        <rdfs:label xml:lang="es">Hechos</rdfs:label>
