        <terms:creator>Jain, Sarika</terms:creator>
        <terms:creator>Mihindukulasooriya, Nandana</terms:creator>
        <terms:description xml:lang="en">An ontology that represents concepts in the legal domain.</terms:description>
        <terms:description xml:lang="de">Eine Ontologie, die Konzepte im Rechtsbereich darstellt.</terms:description>
        <terms:description xml:lang="es">Una ontología que representa conceptos en el dominio jurídico.</terms:description>
        <terms:description xml:lang="fr">Une ontologie qui représente des concepts du domaine juridique.</terms:description>
        <terms:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
        <terms:modified>2020-07-16</terms:modified>
        <terms:title xml:lang="en">NyOn: A Multilingual Legal Ontology for Globalized Judicial System</terms:title>
        <terms:title xml:lang="de">NyOn: Eine mehrsprachige Rechtsontologie für ein globalisiertes Justizsystem</terms:title>
